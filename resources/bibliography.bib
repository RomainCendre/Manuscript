journal = {Scientific Reports},
number = {1},
pages = {1--9},
pmid = {31653943},
title = {{A Curriculum Learning Strategy to Enhance the Accuracy of Classification of Various Lesions in Chest-PA X-ray Screening for Pulmonary Abnormalities}},
volume = {9},
year = {2019}
}
