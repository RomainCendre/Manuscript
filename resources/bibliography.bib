@article{Campos-do-Carmo2008,
abstract = {Dermoscopy is a very useful technique for the analysis of pigmented skin lesions. It represents a link between clinical and histological views, permitting an earlier diagnosis of skin melanoma. It also helps in the diagnosis of many other pigmented skin lesions, such as seborrheic keratosis, pigmented basal cell carcinoma, hemangioma, blue nevus, atypical nevus, and mole, which can often clinically simulate melanoma. In this article, dermoscopy is reviewed from its history to the basic concepts of the interpretation of dermoscopic images. The goal is to introduce this subject to those not yet familiar with it, in order to instigate and encourage the training and practice of this technique of growing importance for everyday usage.},
author = {Campos-do-Carmo, Gabriella and Ramos-e-Silva, Marcia},
