file = {:C$\backslash$:/Users/romai/Documents/Mendeley Desktop/FirstOrder1.pdf:pdf},
journal = {International Journal of Computer Science and Informatioan Security},
keywords = {classification,dense and high,e,extraction,fatty,feature,into four categories i,mammogram,radiologist with,results are compared to,seven parameters are classified,the assessment by the,thereafter,uncompressed fatty},
