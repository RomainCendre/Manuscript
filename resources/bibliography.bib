issn = {00015555},
journal = {Acta Dermato-Venereologica},
keywords = {Epidermis,Human,Regional differences,Skin biopsies,Stratum corneum},
number = {6},
pages = {410--413},
pmid = {14690333},
title = {{Epidermal Thickness at Different Body Sites: Relationship to Age, Gender, Pigmentation, Blood Content, Skin Type and Smoking Habits}},
volume = {83},
year = {2003}
}
